{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "SMTP_HOST": "@smtp_host",
    "SMTP_PORT": "@smtp_port",
    "SMTP_SECURE": "@smtp_secure",
    "SMTP_USER": "@smtp_user",
    "SMTP_PASS": "@smtp_pass",
    "SMTP_FROM": "@smtp_from",
    "MODEL_GENERATION_API_URL": "@model_generation_api_url",
    "MODEL_GENERATION_API_KEY": "@model_generation_api_key",
    "STORAGE_BUCKET": "@storage_bucket",
    "STORAGE_REGION": "@storage_region",
    "STORAGE_ACCESS_KEY": "@storage_access_key",
    "STORAGE_SECRET_KEY": "@storage_secret_key"
  }
} 